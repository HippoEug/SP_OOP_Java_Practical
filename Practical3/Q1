			System.out.println("Congragulations");
		}
		else {
			System.out.println("Wrong, thanks tho");
		}
		*/
		
		switch (option.toLowerCase()) {
		case "no": {
			System.out.println("Congragulations");
			break;
		}
		case "yes": {
			System.out.println("Wrong, thanks tho");
		}
		}
		
		sc.close();
	}
}
